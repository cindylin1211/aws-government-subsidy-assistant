{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowMaggieListBucket",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::640223110023:user/maggieyj"
      },
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::tw-dgr-gov-funding",
      "Condition": {
        "StringLike": {
          "s3:prefix": "dgr-gov-funding/*"
        }
      }
    },
    {
      "Sid": "AllowMaggieObjectAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::640223110023:user/maggieyj"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::tw-dgr-gov-funding/dgr-gov-funding/*"
    }
  ]
}
